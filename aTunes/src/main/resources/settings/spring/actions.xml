<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	
	default-autowire="byName" default-lazy-init="true" default-init-method="initialize">
	
	<bean id="addAlbumMostPlayedAction1" class="net.sourceforge.atunes.kernel.actions.AddAlbumsMostPlayedAction">
		<property name="albums" value="1"/>
		<property name="i18nKey" value="ADD_ALBUM_MOST_PLAYED"/>
	</bean>
	<bean id="addAlbumMostPlayedAction5" class="net.sourceforge.atunes.kernel.actions.AddAlbumsMostPlayedAction">
		<property name="albums" value="5"/>
		<property name="i18nKey" value="ADD_5_ALBUMS_MOST_PLAYED"/>
	</bean>
	<bean id="addAlbumMostPlayedAction10" class="net.sourceforge.atunes.kernel.actions.AddAlbumsMostPlayedAction">
		<property name="albums" value="10"/>
		<property name="i18nKey" value="ADD_10_ALBUMS_MOST_PLAYED"/>
	</bean>
	
	<bean id="addArtistsMostPlayedAction1" class="net.sourceforge.atunes.kernel.actions.AddArtistsMostPlayedAction">
		<property name="artists" value="1"/>
		<property name="i18nKey" value="ADD_ARTIST_MOST_PLAYED"/>
	</bean>
	<bean id="addArtistsMostPlayedAction5" class="net.sourceforge.atunes.kernel.actions.AddArtistsMostPlayedAction">
		<property name="artists" value="5"/>
		<property name="i18nKey" value="ADD_5_ARTISTS_MOST_PLAYED"/>
	</bean>
	<bean id="addArtistsMostPlayedAction10" class="net.sourceforge.atunes.kernel.actions.AddArtistsMostPlayedAction">
		<property name="artists" value="10"/>
		<property name="i18nKey" value="ADD_10_ARTISTS_MOST_PLAYED"/>
	</bean>

	<bean class="net.sourceforge.atunes.kernel.actions.AddPodcastFeedAction"/>
	
	<bean class="net.sourceforge.atunes.kernel.actions.AddRadioAction"/>

	<bean id="addRandomSongsAction10" class="net.sourceforge.atunes.kernel.actions.AddRandomSongsAction">
		<property name="songs" value="10"/>
		<property name="i18nKey" value="ADD_10_RANDOM_SONGS"/>
	</bean> 
	<bean id="addRandomSongsAction50" class="net.sourceforge.atunes.kernel.actions.AddRandomSongsAction">
		<property name="songs" value="50"/>
		<property name="i18nKey" value="ADD_50_RANDOM_SONGS"/>
	</bean> 
	<bean id="addRandomSongsAction100" class="net.sourceforge.atunes.kernel.actions.AddRandomSongsAction">
		<property name="songs" value="100"/>
		<property name="i18nKey" value="ADD_100_RANDOM_SONGS"/>
	</bean> 

	<bean id="addSongsMostPlayedAction10" class="net.sourceforge.atunes.kernel.actions.AddSongsMostPlayedAction">
		<property name="songs" value="10"/>
		<property name="i18nKey" value="ADD_10_SONGS_MOST_PLAYED"/>
	</bean> 
	<bean id="addSongsMostPlayedAction50" class="net.sourceforge.atunes.kernel.actions.AddSongsMostPlayedAction">
		<property name="songs" value="50"/>
		<property name="i18nKey" value="ADD_50_SONGS_MOST_PLAYED"/>
	</bean> 
	<bean id="addSongsMostPlayedAction100" class="net.sourceforge.atunes.kernel.actions.AddSongsMostPlayedAction">
		<property name="songs" value="100"/>
		<property name="i18nKey" value="ADD_100_SONGS_MOST_PLAYED"/>
	</bean> 

	<bean id="addUnplayedSongsAction10" class="net.sourceforge.atunes.kernel.actions.AddUnplayedSongsAction">
		<property name="songs" value="10"/>
		<property name="i18nKey" value="ADD_10_UNPLAYED_SONGS"/>
	</bean> 
	<bean id="addUnplayedSongsAction50" class="net.sourceforge.atunes.kernel.actions.AddUnplayedSongsAction">
		<property name="songs" value="50"/>
		<property name="i18nKey" value="ADD_50_UNPLAYED_SONGS"/>
	</bean> 
	<bean id="addUnplayedSongsAction100" class="net.sourceforge.atunes.kernel.actions.AddUnplayedSongsAction">
		<property name="songs" value="100"/>
		<property name="i18nKey" value="ADD_100_UNPLAYED_SONGS"/>
	</bean> 

	<bean class="net.sourceforge.atunes.kernel.actions.AutoScrollPlayListAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.CheckUpdatesAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.CloseOtherPlaylistsAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.ClosePlaylistAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.CollapseTreesAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.ConnectDeviceAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.CopyPlayListToDeviceAction">
		<property name="playListObjectFilter">
			<bean class="net.sourceforge.atunes.kernel.modules.playlist.PlayListLocalAudioObjectFilter" scope="prototype"/>
		</property>
	</bean>
	<bean class="net.sourceforge.atunes.kernel.actions.CustomSearchAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.DisconnectDeviceAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.EditPreferencesAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.ExitAction"/>
	<bean class="net.sourceforge.atunes.kernel.actions.ExpandTreesAction"/>
</beans>
