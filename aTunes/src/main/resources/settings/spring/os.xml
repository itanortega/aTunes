<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	
	default-autowire="byName" default-lazy-init="true">
	
	<bean id="osManager" class="net.sourceforge.atunes.kernel.OsManager" init-method="initialize"/>

	<bean id="macOsApplication" class="net.sourceforge.atunes.kernel.modules.os.macosx.MacOSXApplication" scope="prototype"/>
	<bean id="macOsDockMenu" class="net.sourceforge.atunes.kernel.modules.os.macosx.MacOSXDockMenu" scope="prototype"/>
	<bean id="macOsInitializer" class="net.sourceforge.atunes.kernel.modules.os.macosx.MacOSXInitializer" scope="prototype"/>

</beans>
