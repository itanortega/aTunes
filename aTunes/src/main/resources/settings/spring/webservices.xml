<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"

	default-autowire="byName" default-lazy-init="true">

	<bean id="lastFmService"
		class="net.sourceforge.atunes.kernel.modules.webservices.lastfm.LastFmService">
		<constructor-arg ref="state" />
	</bean>

	<bean id="lyricsService"
		class="net.sourceforge.atunes.kernel.modules.webservices.lyrics.LyricsService">
		<constructor-arg ref="state" />
		
		
	</bean>

	<bean id="youtubeService"
		class="net.sourceforge.atunes.kernel.modules.webservices.youtube.YoutubeService">
		<constructor-arg ref="state" />
	</bean>

</beans>